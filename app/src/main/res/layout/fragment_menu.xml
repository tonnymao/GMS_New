<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.MenuFragment">

    <!-- TODO: Update blank fragment layout -->

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/imageButton32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/salestracking" />

            <ImageButton
                android:id="@+id/imageButton33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/salestracking"
                app:layout_constraintLeft_toRightOf="@+id/imageButton34"
                android:layout_marginLeft="0dp"
                app:layout_constraintTop_toTopOf="@+id/imageButton34"
                android:layout_marginTop="0dp" />

            <ImageButton
                android:id="@+id/imageButton34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/salestracking"
                app:layout_constraintTop_toTopOf="@+id/imageButton32"
                android:layout_marginTop="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageButton32"
                android:layout_marginBottom="0dp"
                app:layout_constraintLeft_toRightOf="@+id/imageButton32"
                android:layout_marginLeft="0dp"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Sales Tracking"
                android:textAlignment="center"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/imageButton32"
                app:layout_constraintRight_toLeftOf="@+id/imageButton34"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageButton32" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Omzet"
                android:textAlignment="center"
                app:layout_constraintLeft_toRightOf="@+id/imageButton34"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="@+id/imageButton33"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageButton33" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Sales Schedule"
                android:textAlignment="center"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageButton34"
                android:layout_marginLeft="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/imageButton34"
                android:layout_marginRight="8dp"
                app:layout_constraintRight_toRightOf="@+id/imageButton34" />
        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</FrameLayout>
